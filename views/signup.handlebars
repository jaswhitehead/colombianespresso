<body>



    <form class="clearfix mb-4" action="/api/signup" method="POST">
        <div class="form-group">
            <h4>Create Your User:</h4>
            <br>
            <label for="username">
                <h6><strong>Username:</strong></h6>
            </label>
            <input type="text" id="username" class="form-control" aria-describedby="example-text"
                placeholder="ex. User123">
        </div>
        <div class="form-group">
            <label for="user_password">
                <h6><strong>Password:</strong></h6>
            </label>
            <input type="text" id="password" class="form-control" aria-describedby="example-text"
                >
        </div>
        <button onclick="myFunction()" id="submit-user" class="btn btn-primary float-right">Submit</button>

    </form>



<script>
function myFunction() {
  location.replace("/login")
}
</script>
</body>
