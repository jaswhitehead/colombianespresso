  <!-- Start of the News Feed Code -->
  <div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h2>Welcome Back to the Coffee House!</h2>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <h4>Share Your Thoughts</h4>
            <input type="text" id="post-title" class="form-control" aria-describedby="example-text" placeholder="Enter Post Title Here">
          </div>
          <div class="form-group">
            <h6><strong>Enter Post</strong></h6>
            <textarea class="form-control" id="post-body" aria-describedby="example-description" placeholder="What's On Your Mind?"></textarea>
          </div>
          <div class="form-group">
              <label for="category">Select User:</label>
              <select class="custom-select" id="User">
              </select>
            </div>
          <button id="submit" class="btn btn-primary float-right">Submit</button>
        </form>
        <h3>News Feed:</h3>
        <ul id="example-list" class="list-group">
          {{#each post}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="post/{{this.id}}">{{this.post_title}}</a>
            <p>{{this.post_body}}</p>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

